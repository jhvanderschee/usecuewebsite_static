	"defaults": [{"scope": {"path": ""},"values": {"layout": "page","language": "en"}},{"scope": {"path": "nl"},"values": {"layout": "page","language": "nl"}},{"scope": {"path": "_posts/en"},"values": {"layout": "post","language": "en","permalink": "blog/:title/"}},{"scope": {"path": "_posts/nl"},"values": {"layout": "post","language": "nl","permalink": "nl/blog/:title/"}},{"scope": {"path": "_portfolio/en"},"values": {"layout": "portfolio","language": "en","permalink": "portfolio/:title/","_comments": {"image": "desktop 1236 x 782","mobile_image": "mobile 419 x 703,5"}}},{"scope": {"path": "_portfolio/nl"},"values": {"layout": "portfolio","language": "nl","permalink": "nl/portfolio/:title/","_comments": {"image": "desktop 1236 x 782","mobile_image": "mobile 419 x 703,5"}}}],
	
	
	
